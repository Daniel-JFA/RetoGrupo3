{
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "html",
                "htmlWhitespaceSensitivity": "css"
            }
        }
    ]
}
